<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="506.0" prefWidth="707.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.build.lab8.controllers.registrationController">
   <children>
      <Label fx:id="label1" layoutX="328.0" layoutY="77.0" text="Логин">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </Label>
      <TextField fx:id="login" layoutX="252.0" layoutY="115.0" prefHeight="26.0" prefWidth="203.0" />
      <Label fx:id="label2" layoutX="322.0" layoutY="196.0" text="Пароль">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </Label>
      <TextField fx:id="password" layoutX="252.0" layoutY="234.0" prefHeight="26.0" prefWidth="203.0" />
      <Button fx:id="enter" layoutX="290.0" layoutY="326.0" mnemonicParsing="false" onAction="#onRegClick" text="Регистрация">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="310.0" layoutY="384.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Отмена">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="loginLabelError" alignment="CENTER" layoutX="136.0" layoutY="157.0" prefHeight="38.0" prefWidth="434.0" textFill="#c71818">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </Label>
      <Label fx:id="passwordLabelError" alignment="CENTER" layoutX="136.0" layoutY="273.0" prefHeight="38.0" prefWidth="434.0" textFill="#c71818">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </Label>
   </children>
</AnchorPane>
